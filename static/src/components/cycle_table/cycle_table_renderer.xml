<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="steril_supervisorio_dashboard.CycleTableRenderer" owl="1">
        
        <t t-if="props.data">

            
            
            <table class="table table-striped">
                <thead>
                    
                    <tr>
                        <th>Status</th>
                        <th>Cod Carga</th>
                        <th>Operador</th>
                        <th>Inicio</th>
                        <th>Fim</th>
                        <th>Duração</th>
                    </tr>
                </thead>
                <tbody>
            <t t-foreach="props.data" t-as="line" t-key="line.id" >
                <tr>
                    <td>
                    <t t-esc="line.state" />
                   </td>
                    <td> <t t-esc="line.name" /></td>
                    <td> <t t-esc="line.operator" /></td>
                    <td> <t t-esc="line.data_inicio" /></td>
                    <td> <t t-esc="line.data_fim" /></td>
                    <td> <t t-esc="line.duration" /></td>
                </tr>
               
                
            </t>
            
    
    
                </tbody>
            </table>
        </t>
        
    </t>
</templates>